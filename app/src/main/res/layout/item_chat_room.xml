<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:text="chatRoomId"
            android:layout_height="wrap_content"
            android:id="@+id/chatRoomId"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/chatRoomLastMessage"
            android:layout_width="wrap_content"
            android:text="conversationLastMessage"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@+id/chatRoomId"
            app:layout_constraintStart_toStartOf="@+id/chatRoomId"
            app:layout_constraintTop_toBottomOf="@+id/chatRoomId" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>